{
  "name": "pitermarxboilerplate",
  "version": "1.0.0",
  "scripts": {
    "start": "webpack-dev-server",
    "build": "webpack",
    "deploy": "surge -p ./public -d pitermarx-boilerplate.surge.sh",
    "yolo": "npm run build && npm run deploy"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      [
        "transform-react-jsx",
        {
          "pragma": "h"
        }
      ],
      "h-children-fix"
    ]
  },
  "dependencies": {
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-h-children-fix": "^1.1.0",
    "babel-plugin-transform-react-jsx": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "css-loader": "^0.23.1",
    "hjs-webpack": "^6.1.0",
    "postcss-loader": "^0.8.0",
    "redux": "^3.0.5",
    "style-loader": "^0.13.0",
    "stylus-loader": "^1.4.3",
    "surge": "^0.17.4",
    "vdom-to-html": "^2.2.0",
    "vdom-virtualize": "^1.0.1",
    "virtual-dom": "^2.1.1",
    "webpack": "^1.12.10",
    "webpack-dev-server": "^1.14.0",
    "yeticss": "^7.0.5"
  }
}
